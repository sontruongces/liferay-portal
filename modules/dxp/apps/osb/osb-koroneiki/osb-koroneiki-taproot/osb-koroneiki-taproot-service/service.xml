<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_2_0.dtd">

<service-builder mvcc-enabled="true" package-path="com.liferay.osb.koroneiki.taproot">
	<namespace>Koroneiki</namespace>
	<entity local-service="true" name="Account" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="accountId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="accountKey" type="String" />
		<column name="parentAccountId" type="long" />
		<column name="name" type="String" />
		<column name="code" type="String" />
		<column name="description" type="String" />
		<column name="logoId" type="long" />
		<column name="contactEmailAddress" type="String" />
		<column name="profileEmailAddress" type="String" />
		<column name="phoneNumber" type="String" />
		<column name="faxNumber" type="String" />
		<column name="website" type="String" />
		<column name="tier" type="String" />
		<column name="region" type="String" />
		<column name="internal" type="boolean" />
		<column name="status" type="String" />

		<!-- Finder methods -->

		<finder name="AccountKey" return-type="Account" unique="true">
			<finder-column name="accountKey" />
		</finder>
		<finder name="ParentAccountId" return-type="Collection">
			<finder-column name="parentAccountId" />
		</finder>
		<finder name="Name" return-type="Account">
			<finder-column name="name" />
		</finder>
		<finder name="Code" return-type="Account">
			<finder-column name="code" />
		</finder>
	</entity>
	<entity local-service="true" name="AccountNote" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="accountNoteId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="creatorOktaId" type="String" />
		<column name="creatorName" type="String" />
		<column name="modifiedDate" type="Date" />
		<column name="modifierOktaId" type="String" />
		<column name="modifierName" type="String" />

		<!-- Other fields -->

		<column name="accountNoteKey" type="String" />
		<column name="accountId" type="long" />
		<column name="type" type="String" />
		<column name="priority" type="int" />
		<column name="content" type="String" />
		<column name="format" type="String" />
		<column name="status" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="priority" />
			<order-column name="createDate" order-by="desc" />
		</order>

		<!-- Finder methods -->

		<finder name="AccountNoteKey" return-type="AccountNote">
			<finder-column name="accountNoteKey" />
		</finder>
		<finder name="AccountId" return-type="Collection">
			<finder-column name="accountId" />
		</finder>
		<finder name="AI_T_S" return-type="Collection">
			<finder-column name="accountId" />
			<finder-column arrayable-operator="OR" name="type" />
			<finder-column arrayable-operator="OR" name="status" />
		</finder>
	</entity>
	<entity local-service="true" name="Contact" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="contactId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="contactKey" type="String" />
		<column name="oktaId" type="String" />
		<column name="firstName" type="String" />
		<column name="middleName" type="String" />
		<column name="lastName" type="String" />
		<column name="emailAddress" type="String" />
		<column name="languageId" type="String" />
		<column name="emailAddressVerified" type="boolean" />

		<!-- Finder methods -->

		<finder name="ContactKey" return-type="Contact" unique="true">
			<finder-column name="contactKey" />
		</finder>
		<finder name="OktaId" return-type="Contact">
			<finder-column name="oktaId" />
		</finder>
		<finder name="EmailAddress" return-type="Contact">
			<finder-column name="emailAddress" />
		</finder>
	</entity>
	<entity local-service="true" name="ContactAccountRole" remote-service="true">

		<!-- PK fields -->

		<column name="contactId" primary="true" type="long" />
		<column name="accountId" primary="true" type="long" />
		<column name="contactRoleId" primary="true" type="long" />

		<!-- Finder methods -->

		<finder name="ContactId" return-type="Collection">
			<finder-column name="contactId" />
		</finder>
		<finder name="AccountId" return-type="Collection">
			<finder-column name="accountId" />
		</finder>
		<finder name="ContactRoleId" return-type="Collection">
			<finder-column name="contactRoleId" />
		</finder>
		<finder name="CI_AI" return-type="Collection">
			<finder-column name="contactId" />
			<finder-column name="accountId" />
		</finder>
	</entity>
	<entity local-service="true" name="ContactRole" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="contactRoleId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="contactRoleKey" type="String" />
		<column accessor="true" name="name" type="String" />
		<column name="description" type="String" />
		<column name="type" type="String" />
		<column name="system" type="boolean" />

		<!-- Order -->

		<order by="asc">
			<order-column name="name" />
		</order>

		<!-- Finder methods -->

		<finder name="ContactRoleKey" return-type="ContactRole" unique="true">
			<finder-column name="contactRoleKey" />
		</finder>
		<finder name="Type" return-type="Collection">
			<finder-column name="type" />
		</finder>
		<finder name="N_T" return-type="ContactRole">
			<finder-column name="name" />
			<finder-column name="type" />
		</finder>
	</entity>
	<entity local-service="true" name="ContactTeamRole" remote-service="true">

		<!-- PK fields -->

		<column name="contactId" primary="true" type="long" />
		<column name="teamId" primary="true" type="long" />
		<column name="contactRoleId" primary="true" type="long" />

		<!-- Finder methods -->

		<finder name="ContactId" return-type="Collection">
			<finder-column name="contactId" />
		</finder>
		<finder name="TeamId" return-type="Collection">
			<finder-column name="teamId" />
		</finder>
		<finder name="ContactRoleId" return-type="Collection">
			<finder-column name="contactRoleId" />
		</finder>
		<finder name="CI_TI" return-type="Collection">
			<finder-column name="contactId" />
			<finder-column name="teamId" />
		</finder>
	</entity>
	<entity local-service="true" name="Team" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="teamId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="teamKey" type="String" />
		<column name="accountId" type="long" />
		<column name="name" type="String" />
		<column name="system" type="boolean" />

		<!-- Finder methods -->

		<finder name="TeamKey" return-type="Team" unique="true">
			<finder-column name="teamKey" />
		</finder>
		<finder name="AccountId" return-type="Collection">
			<finder-column name="accountId" />
		</finder>
		<finder name="AI_N" return-type="Team">
			<finder-column name="accountId" />
			<finder-column name="name" />
		</finder>
		<finder name="AI_S" return-type="Team">
			<finder-column name="accountId" />
			<finder-column name="system" />
		</finder>
	</entity>
	<entity local-service="true" name="TeamAccountRole" remote-service="true">

		<!-- PK fields -->

		<column name="teamId" primary="true" type="long" />
		<column name="accountId" primary="true" type="long" />
		<column name="teamRoleId" primary="true" type="long" />

		<!-- Finder methods -->

		<finder name="TeamId" return-type="Collection">
			<finder-column name="teamId" />
		</finder>
		<finder name="AccountId" return-type="Collection">
			<finder-column name="accountId" />
		</finder>
		<finder name="TeamRoleId" return-type="Collection">
			<finder-column name="teamRoleId" />
		</finder>
		<finder name="TI_AI" return-type="Collection">
			<finder-column name="teamId" />
			<finder-column name="accountId" />
		</finder>
	</entity>
	<entity local-service="true" name="TeamRole" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="teamRoleId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="teamRoleKey" type="String" />
		<column accessor="true" name="name" type="String" />
		<column name="description" type="String" />
		<column name="type" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="name" />
		</order>

		<!-- Finder methods -->

		<finder name="TeamRoleKey" return-type="TeamRole" unique="true">
			<finder-column name="teamRoleKey" />
		</finder>
		<finder name="Type" return-type="Collection">
			<finder-column name="type" />
		</finder>
		<finder name="N_T" return-type="TeamRole">
			<finder-column name="name" />
			<finder-column name="type" />
		</finder>
	</entity>
	<exceptions>
		<exception>AccountCode</exception>
		<exception>AccountName</exception>
		<exception>AccountParent</exception>
		<exception>ContactEmailAddress</exception>
		<exception>ContactOktaId</exception>
		<exception>ContactRoleName</exception>
		<exception>ContactRoleSystem</exception>
		<exception>ContactRoleType</exception>
		<exception>RequiredAccount</exception>
		<exception>TeamName</exception>
		<exception>TeamRoleName</exception>
		<exception>TeamRoleType</exception>
	</exceptions>
</service-builder>